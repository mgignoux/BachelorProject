function fh = SO3FFT(f,R,L)
[~,Q] = size(R);
fh = zeros(2*L+1,2*L+1,2*L+1);

for n = -L:L
    for i = -n:n
        for j = -n:n
            for q = 1:Q
                alpha = R(1,q);
                beta = R(2,q);
                gamma = R(3,1);
                fh(n+L+1,i+n+1,j+n+1) = fh(n+L+1,i+n+1,j+n+1) + f(alpha,beta,gamma).*WignerD(alpha,beta,gamma,n,i,j);
            end
        end
    end
end
